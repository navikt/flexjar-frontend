{
    "name": "flexjar-frontend",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": ">=20"
    },
    "dependencies": {
        "@faker-js/faker": "^9.0.3",
        "@navikt/aksel-icons": "^7.32.3",
        "@navikt/ds-css": "^7.32.3",
        "@navikt/ds-react": "^7.32.3",
        "@navikt/ds-tailwind": "^7.32.3",
        "@navikt/ds-tokens": "^7.4.0",
        "@navikt/next-api-proxy": "^4.1.5",
        "@navikt/next-logger": "^1.35.0",
        "@navikt/oasis": "^3.8.0",
        "@tanstack/react-query": "^5.90.6",
        "@tanstack/react-table": "^8.21.3",
        "classnames": "^2.5.1",
        "dayjs": "^1.11.18",
        "nextleton": "^0.6.1",
        "nuqs": "^2.7.2",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "tailwind-merge": "^2.6.0",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@axe-core/playwright": "^4.11.0",
        "@navikt/eslint-config-teamsykmelding": "^7.1.0",
        "@playwright/test": "^1.48.2",
        "@types/node": "^22.7.5",
        "@types/react": "18.3.12",
        "@types/react-dom": "18.3.1",
        "@types/uuid": "^10.0.0",
        "autoprefixer": "^10.4.21",
        "eslint": "^8.57.1",
        "eslint-config-next": "15.5.4",
        "next": "15.3.5",
        "pino-pretty": "^13.1.2",
        "playwright-core": "^1.56.1",
        "prettier": "^3.6.2",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.9.3"
    },
    "scripts": {
        "dev": "MOCK_BACKEND=true next dev --turbopack -p 3000 | pino-pretty",
        "local": "LOCAL_BACKEND=true next dev -p 3000 | pino-pretty",
        "e2e": "playwright test",
        "e2e:dev": "yarn e2e --ui",
        "start": "next start -p 3000",
        "build": "next build",
        "prettier:write": "prettier --write .",
        "prettier:check": "prettier --check .",
        "lint": "eslint --ext=ts,tsx src",
        "lint:fix": "eslint --ext=ts,tsx src --fix",
        "format": "npm run prettier:write && npm run lint:fix"
    },
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ],
        "rules": {
            "postcss-modules/no-undef-class": "off",
            "postcss-modules/no-unused-class": "off"
        }
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "overrides": {
        "@types/react": "18.3.12",
        "@types/react-dom": "18.3.1"
    }
}
