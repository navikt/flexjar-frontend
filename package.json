{
    "name": "flexjar-frontend",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": ">=20"
    },
    "dependencies": {
        "@faker-js/faker": "^8.4.1",
        "@navikt/ds-css": "^6.1.1",
        "@navikt/ds-react": "^6.1.1",
        "@navikt/ds-tailwind": "^6.1.1",
        "@navikt/ds-tokens": "^6.1.0",
        "@navikt/next-api-proxy": "^3.4.0",
        "@navikt/next-logger": "^1.23.0",
        "@tanstack/react-query": "^5.26.3",
        "@tanstack/react-table": "^8.13.2",
        "classnames": "^2.5.1",
        "dayjs": "^1.11.10",
        "next-usequerystate": "^1.17.1",
        "nextleton": "^0.6.1",
        "openid-client": "^5.6.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwind-merge": "^2.2.1",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@axe-core/playwright": "^4.8.5",
        "@navikt/eslint-config-teamsykmelding": "^5.1.0",
        "@playwright/test": "^1.42.1",
        "@types/node": "^20.11.24",
        "@types/react": "^18.2.61",
        "@types/react-dom": "^18.2.19",
        "@types/uuid": "^9.0.8",
        "autoprefixer": "^10.4.18",
        "eslint": "^8.57.0",
        "eslint-config-next": "^14.1.0",
        "next": "^14.1.0",
        "pino-pretty": "^10.3.1",
        "playwright-core": "^1.42.0",
        "prettier": "^3.2.5",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.3.3"
    },
    "scripts": {
        "dev": "MOCK_BACKEND=true next dev -p 3000 | pino-pretty",
        "e2e": "playwright test",
        "e2e:dev": "yarn e2e --ui",
        "start": "next start -p 3000",
        "build": "next build",
        "prettier:write": "prettier --write .",
        "prettier:check": "prettier --check .",
        "lint": "eslint --ext=ts,tsx src",
        "lint:fix": "eslint --ext=ts,tsx src --fix",
        "format": "npm run prettier:write && npm run lint:fix"
    },
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ],
        "rules": {
            "postcss-modules/no-undef-class": "off",
            "postcss-modules/no-unused-class": "off"
        }
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier"
}
